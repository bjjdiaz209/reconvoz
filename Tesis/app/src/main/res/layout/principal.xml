<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".MainActivity"
    style="@style/b"
    android:layout_width="match_parent"
    android:background="@drawable/fondo">

    <TextView
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Mesas"
        android:id="@+id/textView5"
        android:textColor="#ffec8605"
        android:textSize="30sp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/home"
        android:layout_alignEnd="@+id/home"
        android:textStyle="bold" />

    <ImageButton
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:id="@+id/home"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:background="@drawable/circulo"
        android:src="@android:drawable/ic_menu_sort_by_size" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/Mesa3"
        android:background="@drawable/mesa"
        android:src="@drawable/cubi"
        android:layout_above="@+id/textView30"
        android:layout_alignRight="@+id/notas"
        android:layout_alignEnd="@+id/notas"
        android:layout_marginRight="19dp"
        android:layout_marginEnd="19dp" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/Mesa2"
        android:background="@drawable/mesa"
        android:src="@drawable/cubi"
        android:layout_below="@+id/textView5"
        android:layout_alignRight="@+id/textView5"
        android:layout_alignEnd="@+id/textView5" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/Mesa1"
        android:background="@drawable/mesa"
        android:src="@drawable/cubi"
        android:layout_below="@+id/textView5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/Mesa5"
        android:background="@drawable/mesa"
        android:src="@drawable/cubi"
        android:layout_above="@+id/textView33"
        android:layout_alignRight="@+id/home"
        android:layout_alignEnd="@+id/home" />

    <ImageButton
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/Mesa4"
        android:background="@drawable/mesa"
        android:src="@drawable/cubi"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="65dp"
        android:layout_height="60dp"
        android:id="@+id/notas"
        android:background="@drawable/notas"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Mesa 1"
        android:id="@+id/textView29"
        android:textColor="#ffb2b2a3"
        android:layout_below="@+id/Mesa1"
        android:layout_alignRight="@+id/Mesa1"
        android:layout_alignEnd="@+id/Mesa1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Mesa 2"
        android:id="@+id/textView30"
        android:textColor="#ffb2b2a3"
        android:layout_below="@+id/Mesa2"
        android:layout_alignLeft="@+id/textView33"
        android:layout_alignStart="@+id/textView33" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Mesa 3"
        android:id="@+id/textView31"
        android:textColor="#ffb2b2a3"
        android:layout_below="@+id/Mesa3"
        android:layout_alignRight="@+id/Mesa3"
        android:layout_alignEnd="@+id/Mesa3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Mesa 4"
        android:id="@+id/textView32"
        android:textColor="#ffb2b2a3"
        android:layout_below="@+id/Mesa4"
        android:layout_alignRight="@+id/Mesa4"
        android:layout_alignEnd="@+id/Mesa4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Mesa 5"
        android:id="@+id/textView33"
        android:textColor="#ffb2b2a3"
        android:layout_alignTop="@+id/textView32"
        android:layout_alignLeft="@+id/home"
        android:layout_alignStart="@+id/home" />

</RelativeLayout>
